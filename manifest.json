{
  "manifest_version": 2,
  "name": "Sinkless",
  "description": "A sync-less password manager",
  "homepage_url": "http://kybernetikos.github.io/sinkless",
  "version": "1.0.3",
  "browser_action": {
    "default_title": "Generate Password",
    "default_popup": "index.html",
    "default_icon": "img/pass.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"}
    }
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}
